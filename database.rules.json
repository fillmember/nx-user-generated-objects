{
  "rules": {
    ".read": true,
    ".write": "auth != null",
    "objects": {
      ".read": true,
      "$obj_id": {
        ".validate": "newData.hasChildren(['scene_id','user_id','object_id','matrix'])",
        "scene_id": {
          ".validate": "newData.isString() && newData.val().length > 0"
        },
        "user_id": {
          ".validate": "newData.val() == auth.uid"
        },
        "object_id": {
          ".validate": "newData.isString() && newData.val().length > 0"
        },
        "matrix": {
          ".validate": "newData.isString() && newData.val().length > 0"
        },
        "$other": {
          ".validate": false
        }
      }
    }
  }
}
